[user]
	name = leachiM2k
	email = leachim2k@leachim2k.de
[alias]
	logf = log --name-status --no-merges
[status]
	color = auto
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
	ui = true
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
